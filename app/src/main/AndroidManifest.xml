<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.mmlodawski.jvmoverloadsmediumexample">

    <application
        android:allowBackup="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:name=".JvmOverloadsMediumExampleApp"
        android:theme="@style/AppTheme">
        <activity android:name="com.mmlodawski.jvmoverloadsmediumexample.activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity android:name=".activity.ButtonsActivity"/>

        <activity android:name=".activity.TextInputEditTextsActivity"/>

        <activity android:name=".activity.CheckBoxesActivity"/>
    </application>

</manifest>